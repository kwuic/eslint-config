    "./nextjs": "./nextjs.config.mjs",
    "./nextjs-no-tailwind": "./nextjs-no-tailwind.config.mjs",
    "./fastify": "./fastify.config.mjs",
    "./prettier": "./.prettierrc.yaml"
  },
  "files": [
