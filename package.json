    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript-eslint": "^8.20.0"
