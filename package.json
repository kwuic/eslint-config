{
  "name": "@kwuic/eslint-config",
  "version": "0.0.5",
  "description": "Comprehensive ESLint and Prettier configuration for Next.js and Fastify projects with TypeScript",
  "author": "kwuic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwuic/eslint-config.git"
  },
  "homepage": "https://github.com/kwuic/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/kwuic/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "nextjs",
    "react",
    "typescript",
    "fastify",
    "code-quality",
    "linting",
    "formatting"
  ],
  "main": "eslint.config.mjs",
  "exports": {
